
<tr>
  <td>
    <div class="toggle">
      <label class="toggle-radio" for="toggleOption<%=upload.id %>-2">ON</label>
      <input type="checkbox" name="mod[]" id="toggleOption<%= upload.id %>-1" value="<%= upload.id %>">
      <label class="toggle-radio" for="toggleOption<%=upload.id %>-1">OFF</label>
      <input type="checkbox" id="toggleOption<%= upload.id %>-2" value="option2" checked="checked">
    </div>
  </td>
  <td><pre><%= upload.mod_jar_file_name %></pre></td>
  <td><div class="span2"><%= link_to 'Show', upload, :class => "btn btn-large btn-primary btn-block" %></div></td>
  <td><div class="span2"><%= link_to 'Edit', edit_upload_path(upload), :class => "btn btn-large btn-primary btn-block" %></div></td>
  <td><div class="span2"><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-large btn-warning btn-block" %></div></td>
</tr>
 
